{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Django",
      "type": "debugpy",
      "request": "launch",
      "args": [
        "runserver",
        "8000"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py",
      "envFile": "${workspaceFolder}/.envs/.local/.django",
      "env": {
        "DATABASE_URL": "postgres:///validibot"
      },
      "justMyCode": false
    },
    {
      "name": "Python Debugger: Django (Docker Postgres)",
      "type": "debugpy",
      "request": "launch",
      "args": [
        "runserver",
        "8000"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py",
      "envFile": "${workspaceFolder}/.envs/.local/.django",
      "env": {
        "DATABASE_URL": "postgres://validibot:validibot@localhost:5432/validibot"
      },
      "justMyCode": false
    }
  ]
}
